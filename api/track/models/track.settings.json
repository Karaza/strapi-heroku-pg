{
  "kind": "collectionType",
  "collectionName": "tracks",
  "info": {
    "name": "track"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 1
    },
    "slug": {
      "type": "string",
      "unique": true,
      "minLength": 1,
      "required": true
    },
    "secondaryTitle": {
      "type": "string"
    },
    "tertiaryTitle": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "rating": {
      "type": "integer",
      "min": 0,
      "required": true
    },
    "duration": {
      "type": "decimal",
      "min": 0,
      "required": true
    },
    "numberOfVersions": {
      "type": "integer",
      "min": 1
    },
    "bpm": {
      "type": "integer",
      "min": 1
    },
    "googleID": {
      "type": "string",
      "unique": false
    },
    "ShortVersionsDuration": {
      "type": "string"
    },
    "tags": {
      "type": "string"
    }
  }
}
